<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="lazyLoadingEnabled" value="true"/>
		<setting name="aggressiveLazyLoading" value="true"/>
	</settings>
	
	<!-- 别名配置 -->
	<typeAliases>
		<!-- <typeAlias alias="ParamS2S" type="com.demo.domain.ParamS2S"/> -->
		<typeAlias alias="User" type="com.user.domain.User"/>
		<typeAlias alias="Param" type="com.param.domain.Param"/>
		<typeAlias alias="Interface" type="com.testing.domain.Interface"/>
		<typeAlias alias="Version" type="com.setting.domain.Version"/>
	</typeAliases>
	
</configuration>